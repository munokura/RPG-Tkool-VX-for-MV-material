":true}]},{"code":505,"indent":0,"parameters":[{"code":3,"indent":null,"parameters":[]}]},{"code":117,"indent":0,"parameters":[1]},{"code":117,"indent":0,"parameters":[6]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["？？？"]},{"code":401,"indent":0,"parameters":["「お怪我は？」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[3]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「ああ、大丈夫だ。おかげで助かったよ」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「俺は光刃。ネストイーターNo.7の、"]},{"code":401,"indent":0,"parameters":["　自分で言うのもなんだけど有名ランカーだ」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「私は滅風と申します」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「……滅びの風か。"]},{"code":401,"indent":0,"parameters":["　コードネームかなんかだと思うが、物騒なネーミングだな」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「任務はいつも殲滅戦でしたから。"]},{"code":401,"indent":0,"parameters":["　敵を皆殺しにすることが、私の生きる道だったんです」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「……ややこしい過去がありそうだな。"]},{"code":401,"indent":0,"parameters":["　しっかし、かわいい顔して過激なこと言うねぇ」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「君みたいな美人には、もっときれいな名前が似合ってるよ。"]},{"code":401,"indent":0,"parameters":["　君も滅風なんて名前はいやだろ？」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「私の名前などこれで十分です」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「いやいや……まだ若いんだから、十分とかもういいとか"]},{"code":401,"indent":0,"parameters":["　言うもんじゃない。年相応に欲張らなくちゃ」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「そうだ、俺が新しい名前を考えてあげるよ。"]},{"code":401,"indent":0,"parameters":["　君の美貌にふさわしい、素敵な名前をね」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「えーと……そうだなぁ……」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「……」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「……」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「……こういうのって、いざ考えようとすると"]},{"code":401,"indent":0,"parameters":["　出てこないんだよな」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「そうですか。ではまた今度お願いします」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「そうだな。"]},{"code":401,"indent":0,"parameters":["　君の用事が終ってからゆっくり考えるとするか」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「……！」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「おっ、ようやく表情が変わったな。"]},{"code":401,"indent":0,"parameters":["　ほんのちょっとだけど」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「味方はともかく、他の組織のランカーはただの商売敵だ。"]},{"code":401,"indent":0,"parameters":["　それをわざわざ助けるなんて、裏があるに決まってる」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「で、何をしてほしいんだ？　暗殺？　護衛？"]},{"code":401,"indent":0,"parameters":["　それともお茶に付き合えばいいのかな？」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「……話が早くて助かります。"]},{"code":401,"indent":0,"parameters":["　断るようなら殺害も辞さないつもりでしたが」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「いやあ、もう殺されたようなもんさ。"]},{"code":401,"indent":0,"parameters":["　君に見つめられるだけで、心臓が止まりそうだ」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「……」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["（赤くなった……いいねえ、このうぶな反応）"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「……で、では、単刀直入にお伝えします」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「私の目的は……Ｂ・Ｒ・Ｋの指導者、"]},{"code":401,"indent":0,"parameters":["　Ｔ・マグワイアの暗殺です」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「……運命を感じるね。俺もあいつを殺りにきたんだ」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「彼は様々な組織の極秘情報を握っている……と、"]},{"code":401,"indent":0,"parameters":["　上の者が言っていました」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「あなたも情報の漏洩を防ぐために？」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「そんなところだな。"]},{"code":401,"indent":0,"parameters":["　しかし、いろんなところの秘密を知ってる、か……」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「こりゃ、他にも結構な数のランカーが"]},{"code":401,"indent":0,"parameters":["　この港未来に送りこまれているだろうな」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「可能ならば、彼らとも共闘したいのですが……」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「無理だろうな。普通、ランカー同士で協力なんかしない」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「俺たちは他のランカーを倒すことでランクが上がり"]},{"code":401,"indent":0,"parameters":["　権力と財力、そして栄誉を得られるんだからな」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「ことの次第によっちゃ、たとえ相手が"]},{"code":401,"indent":0,"parameters":["　同盟を結んだ組織のランカーでも黙認されるくらいだ」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「そこにくると、俺みたいな有名ランカーは"]},{"code":401,"indent":0,"parameters":["　猛獣か金のなる木に見えてるだろうよ」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「逃げるようならほっときゃいいが、"]},{"code":401,"indent":0,"parameters":["　向かってくるなら倒すしかない」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「そう……ですね。"]},{"code":401,"indent":0,"parameters":["　ターゲット以外との交戦も考えておくべきでしょうか」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「まあ、俺みたいな変わり者もいるけどな。"]},{"code":401,"indent":0,"parameters":["　お金と美人にはホント弱いんだ、これが」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「あなたも分からない人ですね。"]},{"code":401,"indent":0,"parameters":["　どこまで本気で喋ってるんです？」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「俺は女の子の前ならいつでも本気さ。"]},{"code":401,"indent":0,"parameters":["　分からないといえば、君の方が分からないよ」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「君はどんな色が好きなのか、どんな音楽が好きなのか、"]},{"code":401,"indent":0,"parameters":["　どんな男が好きなのか……とかね」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「……好きなもの、ですか。考えたこともないですね」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「……ってことは、俺が好みだったから"]},{"code":401,"indent":0,"parameters":["　助けてくれたってわけじゃなかったのか」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「なーんだ、血生臭い戦場で運命の出会い……"]},{"code":401,"indent":0,"parameters":["　ってのも結構乙なもんだと思ったのになぁ」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code":401,"indent":0,"parameters":["「……あなたなら、最後まで信じられると思ったからです」"]},{"code":117,"indent":0,"parameters":[9]},{"code":117,"indent":0,"parameters":[11]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["光刃"]},{"code":401,"indent":0,"parameters":["「……え？」"]},{"code":117,"indent":0,"parameters":[8]},{"code":117,"indent":0,"parameters":[12]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["滅風"]},{"code